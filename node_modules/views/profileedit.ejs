 <script>
$(document).ready(function() {
    $('#Save').click(function(event){

	
        data_phn = $('#id_phone').val();
       
       var number= data_phn.replace(/[^\d]/g, '');
		//alert('in number:'+number.length);
		
		data_altphn = $('#id_altphone').val();
        
       var number_alt= data_altphn.replace(/[^\d]/g, '');
		//alert('in number_alt:'+number_alt.length);
		
		 data_fax = $('#id_fax').val();
		 
       var number_fax= data_fax.replace(/[^\d]/g, '');
		//alert('in number_fax:'+number_fax.length);

		var invalid_number = new Array();
		var invalid=false;
		
	   if(number.length < 9 || number.length >= 12)
	   { 
			invalid_number.push('Phone Number');
			invalid=true;
			
	   }
	   if(number_alt.length < 9 || number_alt.length >= 12)
	   {
			invalid_number.push('Alternate Phone Number');
			invalid=true;
			
	   }
	   if(number_fax.length < 9 || number_fax.length >= 12)
	   {
			invalid_number.push('Fax');
			invalid=true;
				
	   }
	   
	   if(invalid)
	   {
	   var h_v='is';
			   event.preventDefault();
			   if(invalid_number.length>1)
			   {
					var h_v='are';
			   }
			  
			   var err_msg= invalid_number+' '+h_v+' in invalid format. Please enter value having minimum 10 or maximum 12 digits.';
			   document.getElementById("err_pr").innerHTML = err_msg;
			   $('#invalid_format').show();
	   }
	   
	   
	   

    });
});   
 </script> 
