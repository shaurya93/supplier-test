<script>
$(document).ready(function() {
    $('#submit').click(function(event){

        data = $('#new_password').val();
        var len = data.length;
       
        if(len < 1) {
        
            // Prevent form submission
			$('#emptypswd').show();
            event.preventDefault();
			//$(this).addClass('used');
        }
        
        if($('#new_password').val() != $('#confirm_password').val()) {
        
            // Prevent form submission
			$('#nonmatch').show();
            event.preventDefault();
			//$(this).addClass('used');
        }
        
    });
});
</script>

